<div class="container mt-4">
        <div class="row">
            <div class="col-md-4 mx-auto">
                <h1>User Dashboard</h1>
            </div>
        </div>
    </div>

    <div class="container">
        <div class="row">
            <div class="col-6">
                <!--render grocery list data-->
                {{#each lists}}
                <div class="card my-4">
                    <div class="card-header">
                        {{dataValues.name}}
                    </div>
                    <div class="card-body">
                        <p class="text">{{dataValues.items}}</p>
                        <button class="btn btn-primary float-right " type="delete" id="delete-list" data-listID="{{dataValues.id}}">Delete</button>
                        <button class="btn btn-primary float-right mr-2" type="submit" id="edit-list" data-listID="{{dataValues.id}}">Edit</button>
                    </div>
                </div>
                {{/each}}
            </div>
        </div>
    

    </div>